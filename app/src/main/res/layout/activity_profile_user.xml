<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile_User">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="411dp"
        android:layout_height="147dp"
        android:layout_marginTop="80dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/user_wave" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="411dp"
        android:layout_height="88dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="#5B1D93"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="165dp"
        android:layout_height="169dp"
        android:layout_marginTop="88dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/user" />



    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bot_con"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:background="#d2430f">


        <ImageView
            android:id="@+id/home_button"
            android:layout_width="60dp"
            android:layout_height="30dp"
            android:layout_marginLeft="52dp"
            app:layout_constraintBottom_toBottomOf="@id/bot_con"
            app:layout_constraintLeft_toLeftOf="@id/bot_con"
            app:layout_constraintTop_toTopOf="@id/bot_con"
            app:layout_constraintVertical_bias="0.538"
            app:srcCompat="@drawable/ic_baseline_home_24" />

        <ImageView
            android:id="@+id/list_document"
            android:layout_width="60dp"
            android:layout_height="30dp"
            app:srcCompat="@drawable/ic_baseline_view_list_24"
            tools:layout_editor_absoluteX="171dp"
            tools:layout_editor_absoluteY="7dp"
            app:layout_constraintTop_toTopOf="@id/bot_con"
            app:layout_constraintBottom_toBottomOf="@id/bot_con"
            app:layout_constraintLeft_toRightOf="@id/home_button"
            app:layout_constraintRight_toLeftOf="@id/profile"/>

        <ImageView
            android:id="@+id/profile"
            android:layout_width="60dp"
            android:layout_height="30dp"
            android:layout_marginRight="52dp"
            android:background="#5B1D93"
            app:srcCompat="@drawable/ic_baseline_account_circle_24"
            app:layout_constraintTop_toTopOf="@id/bot_con"
            app:layout_constraintBottom_toBottomOf="@id/bot_con"
            app:layout_constraintRight_toRightOf="@id/bot_con"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/email_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:drawableLeft="@drawable/ic_baseline_mark_email_read_24"
        android:drawablePadding="10dp"
        android:text="pallavivishwakarma999@gmail.com"
        android:textColor="@color/black"
        android:layout_marginLeft="20dp"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.184"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <TextView
        android:id="@+id/forget"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:drawableLeft="@drawable/ic_baseline_phonelink_lock_24"
        android:drawablePadding="10dp"
        android:text="Forget Password"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_id" />

    <TextView
        android:id="@+id/document"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:drawableLeft="@drawable/ic_baseline_insert_drive_file_24"
        android:drawablePadding="10dp"
        android:text="My Documents"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/forget" />

    <TextView
        android:id="@+id/logout"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:drawableLeft="@drawable/ic_baseline_add_to_home_screen_24"
        android:drawablePadding="10dp"
        android:text="Logout"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/document" />

</androidx.constraintlayout.widget.ConstraintLayout>